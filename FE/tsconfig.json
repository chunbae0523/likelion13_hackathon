{
  "extends": "../node_modules/expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "types": [
      "react",
      "react-native"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "FE/.expo/types/**/*.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
