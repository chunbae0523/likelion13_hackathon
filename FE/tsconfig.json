{
  "extends": "./node_modules/expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "types": [
      "react",
      "react-native"
    ],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "FE/.expo/types/**/*.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
